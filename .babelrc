{
  "presets": [
    ["@babel/preset-env"]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ],
  "env": {
    "es": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false,
            "loose": true
          }
        ]
      ]
    },
    "commonjs": {
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "add-module-exports"
      ]
    }
  }
}
